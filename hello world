$ sudo apt update
$ sudo apt install git
$ git --version
$ cd
$ mkdir HelloGit
$ cd HelloGit
$ git init
$ echo "Hello World" > hello_world.txt
$ git status
$ git add hello_world.txt
$ git status
$ git commit -m "This is my first commit to Add hello_world.txt"
$ git log
$ git remote add origin https://github.com/<USER>/<REPO>.git
$ git push -u origin master
$ git pull origin master
$ git log
$ git checkout -b develop_feature_A
$ git branch
$ echo "Hello World! This is a branch." > hello_world.txt
$ git add hello_world.txt
$ git checkout master
$ cat hello_world.txt
$ git checkout develop_feature_A
$ git commit
$ git checkout master
$ cat hello_world.txt
$ git checkout develop_feature_A
$ cat hello_world.txt
$ git push origin develop_feature_A
